<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Register user</title>
</head>
<body>
	<form action="/Final/dang-ky" method="post">
		<div class="form-group row">
		
		<label style="color: white; background-color: black;">ID:</label>
			 <input type="text" value="" name="id">
			 </div>
			 <div>
			<label style="color: white; background-color: black;">Name:</label>
			 <input type="text" value="" name="name">
		</div>
		<div>
			<label style="color: white; background-color: black;">Username:</label>
			 <input type="text" value="" name="username">
		</div>
		<div class="form-group">
			<label style="font-size: 20px; font-style: italic;">Password:</label>
			 <input type="password" name="password">
		</div>
		<div class="form-group row">
			<label style="color: white; background-color: black;">Role:</label>
			 <input type="text" value="" name="role">
		</div>
		<div>
			<label style="color: white; background-color: black;">City:</label>
			 <input type="text" value="" name="city">
		</div>
		
		<div class="form-group">
			<label id="gender-label">Gioi tinh:</label> 
			<input type="radio" name="gender" value="F"> Nam 
			<input type="radio" name="gender" value="M"> Nu 
			<input type="radio" name="gender" value="NA"> N/A
		</div>
		<div>
			
			<label style="color: white; background-color: black;">imageName:</label>
			 <input type="text" value="" name="imageName">
		</div>
		<div>
			<button type="submit" class="btn btn-primary">Register</button>
		</div>
	</form>
</body>
</html>